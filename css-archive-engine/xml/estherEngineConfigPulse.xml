         <monitor>0.5</monitor>
      </channel>
      <channel>
         <name>Esther:gas:PT502</name>
         <period>1.0</period>
         <monitor>0.5</monitor>
      </channel>
      <channel>
         <name>Esther:gas:PT601</name>
         <period>1.0</period>
         <monitor>0.5</monitor>
      </channel>
      <channel>
         <name>Esther:gas:PT801</name>
         <period>1.0</period>
         <monitor>1.0</monitor>
      </channel>

      <!-- Channel that is scanned every 1 minutes
      <channel>
         <name>Esther:gas:PT801</name>
         <period>00:01:00</period><scan/>
        -->

      <channel>
         <name>Esther:gas:MFC201_FFLOW</name>
         <period>1.0</period>
         <monitor>0.5</monitor>
      </channel>
      <channel>
         <name>Esther:gas:MFC401_FFLOW</name>
