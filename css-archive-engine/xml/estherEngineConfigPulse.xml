         <monitor>1</monitor>
      </channel>

      <channel>
         <name>Esther:gas:PT901</name>
         <period>00:00:02</period><scan/>
      </channel>
      <channel>
         <name>Esther:gas:CONTROL-ON</name>
         <period>60</period>
         <monitor/>
         <enable/>
      </channel>
      <!-- 
  </group>

  <group>
    <name>Esther MFC Flow/Volumes</name>
