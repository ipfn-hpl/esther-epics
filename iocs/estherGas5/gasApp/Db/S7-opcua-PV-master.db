record(bo, "$(P)$(R)Master_Enable")
{
    field(DESC, "S7PLC Gas master control Enable")
    field(DTYP, "OPCUA")
# monitor y (default)     /output record gets biderectional
    field(OUT,  "@$(SUBS) ns=4;i=89 monitor=y")
    field(ZNAM, "OFF")
    field(ONAM, "ON")
}

