record(ao, "$(P)$(R)PT901-SP-Flush") {
    field(DESC, "S7 PLC Gas PT901 SP Flush")
    field(DTYP, "OPCUA")
    field(OUT,  "@$(SUBS) ns=4;i=37 monitor=n")
    field(LOPR, "0.0")
    field(HOPR, "15.0")
    field(EGU, "Bar")
    field(PREC, "2")
}

