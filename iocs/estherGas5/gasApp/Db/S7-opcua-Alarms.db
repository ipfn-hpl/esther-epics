record(bi, "$(P)$(R)PT101-Alarm")
{
    field(DESC, "S7 PLC PT101 Alarm")
    field(DTYP, "OPCUA")
    field(INP,  "@$(SUBS) ns=4;i=127")
    field(SCAN, "I/O Intr")
    field(ZNAM, "OFF")
    field(ONAM, "ON")    
}
