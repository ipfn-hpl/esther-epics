record(ai,"$(P)$(R)MFC201_FLOW")
{
    field(DESC, "S7 MFC201 O2 FLOW Measure")
    field(DTYP, "OPCUA")
    field(INP,  "@$(SUBS) ns=4;i=47")
    field(SCAN, "I/O Intr")
    field(HOPR, "2.5")
    field(LOPR, "0.0")
    field(HIGH, "1.95")
    field(HIHI, "2.10")
    field(HSV, "MINOR")
    field(HHSV, "MAJOR")
    field(EGU, "m3n/h")
    field(PREC, "2")
    field(ADEL, "0.05")
}
