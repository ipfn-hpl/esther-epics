######### Records of the PLC PT70x LS Alarm Status bits   ############
#
record(bi, "$(user):ALARM_PV701_NOT_CLS")
{
        field(SCAN, "I/O Intr")
        field(DTYP, "s7nodave")
        field(INP, "@myPLC(PG=default) M21.5")
        field(DESC, "S7 PLC PV701 NOT CLOSED ALARM")
        field(ZNAM, "OFF")
        field(ONAM, "ON")
        field(OSV, "MAJOR")
}
