######## Records of the PLC PT Tags - Real-   ############
record(ai,"$(user):PT101")
{
        field(DESC, "S7 PT101 N2 PURGE")
        field(SCAN, "I/O Intr")
        field(DTYP, "s7nodave")
        field(INP, "@myPLC(PG=fast) MD200 float")
        field(HOPR, "250")
        field(LOPR, "-1.0")
	field(PREC,2)
        field(EGU, "Bar")
#        field(LOW, "115")
#        field(LOLO, "15.0")
        field(LOW, "20.0")
        field(LOLO, "10.0")
        field(LSV, "MINOR")
        field(LLSV, "MAJOR")
        field(ADEL, "0.5")
}
record(ao,"$(user):PT101_LOW")
{
        field(DESC, "S7 PT101 N2 PURGE LOW ALARM SP")
        field(DTYP, "s7nodave")
        field(OUT, "@myPLC MD392 float")
        field(LOPR, "-1.0")
        field(HOPR, "250.0")
	field(PREC,2)
        field(EGU, "Bar")
        field(PINI, "YES")
        field(FLNK, "$(user):PT101_LOW_FWD")
}
record(ao,"$(user):PT101_LOW_FWD")
{
        field(DESC, "S7 PT101 LOW FWD")
	field(OMSL, "closed_loop")
        field(DOL, "$(user):PT101_LOW NPP")
  	field(OUT, "$(user):PT101.LOW")
}
