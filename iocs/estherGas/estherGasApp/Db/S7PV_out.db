#########  PLC VALVE command (super user mode) #######
record(bo, "$(user):PV101_OUT")
{
    field(DTYP, "s7nodave")
    field(OUT, "@myPLC Q9.3")
	field(DESC, "S7 PLC PV101 NC OUT")
	field(ZNAM, "CLOSED")
	field(ONAM, "OPEN")
    field(OSV,  "MINOR")
    field(PINI, "YES")
    field(ASG, "critical")
}
