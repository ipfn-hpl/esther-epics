# mbbi record template for register inputs
record(mbbi,"$(P):$(R)-mbbi") {
    field(DTYP,"asynUInt32Digital")
    field(INP,"@asynMask($(PORT) $(OFFSET=0) 0xFFFF)UINT16")
    field(ZRVL,"0")
    field(ZRST,"Bus/RS232")
    field(ONVL,"1")
    field(ONST,"Analog Input")
    field(TWVL,"2")
    field(TWST,"FLOW-BUS slave")
    field(THVL,"3")
    field(THST,"Valve Close")
    field(FRVL,"4")
    field(FRST,"Controller idle")
    field(FVVL,"7")
    field(FVST,"Setpoint 100%")
    field(SXVL,"8")
    field(SXST,"Valve Fully Open")
    field(SVVL,"9")
    field(SVST,"Calibration mode")
    field(EIVL,"10")
    field(EIST,"Analog Slave")
    field(NIVL,"12")
    field(NIST,"Setpoint 0%")
    field(TEVL,"13")
    field(TEST,"FLOW-BUS analog slave")
    field(ELVL,"18")
    field(ELST,"RS232")
    field(TVVL,"20")
    field(TVST,"Valve Steering")
    field(TTVL,"21")
    field(TTST,"Analog valve steering")
    field(FTVL,"22")
    field(FTST,"Valve safe state")
    field(SCAN,"$(SCAN=I/O Intr)")
}
