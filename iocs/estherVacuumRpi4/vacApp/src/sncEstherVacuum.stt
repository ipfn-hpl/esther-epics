assign EDA_DT_LS1_ON to "{P}:EDA:DT-Valve_LS1_ON";
monitor EDA_DT_LS1_ON;

short HVA_CTST_Valve;
assign HVA_CTST_Valve to "{P}:HVA:CTST-Valve";
