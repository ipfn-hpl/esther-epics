            //pvPut(msg);
            //errlogSevPrintf(NO_ALARM, "%s\n", msg);
        }

        when (OPSTATE == POS_Idle) {
        /*
            ARM_CTST_OutCmd = ON;
            pvPut(ARM_CTST_OutCmd);
            ARM_STDT_OutCmd = ON;
