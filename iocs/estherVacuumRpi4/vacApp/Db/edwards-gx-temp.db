# ###   command  EDWARDS GX600 Dry Pumps          ### #
# These records are writen                        ### #
#           by Python gxPumpEpicsPut.py script    ### #
# Water circuit temperature sensor                ### #
# Power is shut-off to the motor if the 
# temperature exceeds 70°C (Warning at 60°C).     ### #
# ###                                             ### #
#######################################################

################################################
# ### Read Parameters                      ### #
################################################
record(ai, $(P)EDW:DryPump-ST-Temp){
    field(DESC, "Holds measured GX600 temperature")
    field(DTYP, "Soft Channel")
    field(LOPR, "5")
    field(HOPR, "70")
#    field(HIGH, "50")
#    field(HIHI, "55")
    field(HHSV,"MAJOR")
    field(HSV, "MINOR")
    field(LOW,  "10")
    field(LOLO, "6")
    field(LLSV,"MAJOR")
    field(LSV, "MINOR")
    field(EGU,  "ºC")
    field(MDEL, "0.5")
    field(ADEL, "1.0")
    info(autosaveFields, "HIGH HIHI")
}
record(ai, $(P)EDW:DryPump-DT-Temp){
    field(DESC, "Holds measured GX600 temperature")
    field(DTYP, "Soft Channel")
    field(LOPR, "5")
    field(HOPR, "70")
#    field(HIGH, "50")
#    field(HIHI, "55")
    field(HHSV,"MAJOR")
    field(HSV, "MINOR")
    field(LOW,  "10")
    field(LOLO, "6")
    field(LLSV,"MAJOR")
    field(LSV, "MINOR")
    field(EGU,  "ºC")
    field(MDEL, "0.5")
    field(ADEL, "1.0")
    info(autosaveFields, "HIGH HIHI")
}

