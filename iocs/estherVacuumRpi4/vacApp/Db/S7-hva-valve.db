#####         PLC  Bit Tags     ############
record(bo, "$(P)$(R):CTST-Valve")
{
        field(DTYP, "s7nodave")
        field(OUT, "@vacPLC Q0.7")
        field(DESC, "S7 PLC CTST HVA VALVE Solenoid")
        field(ZNAM, "OFF")
        field(ONAM, "ON")
        field(VAL, "0")
        field(PINI, "YES")
}
record(bo, "$(P)$(R):STDT-Valve")
{
        field(DTYP, "s7nodave")
        field(OUT, "@vacPLC Q0.6")
        field(DESC, "S7 PLC STDT HVA VALVE Solenoid")
        field(ZNAM, "OFF")
        field(ONAM, "ON")
        field(VAL, "0")
        field(PINI, "YES")
}
